@model IEnumerable<BrandViewModel>

<h2>Бренды</h2>
<div class="brands-name">
    <ul class="nav nav-pills nav-stacked">
        @foreach (var brand in Model)
        {
            <li><a asp-controller="Catalog" asp-action="Index" asp-route-BrandId="@brand.Id"> <span class="pull-right">(--)</span>@brand.Name</a></li>
        }

    </ul>
</div>
